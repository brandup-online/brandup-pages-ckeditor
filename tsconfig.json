{
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable"
    ],
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "strict": true,
    "module": "es6",
    "target": "es2019",
    "sourceMap": true,
    "allowJs": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
    "typings"
  ]
}